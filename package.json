{
    "name": "@reiryoku/mida",
    "version": "1.0.0",
    "description": "A JavaScript framework to easily operate in financial markets.",
    "scripts": {
        "lint": "tslint --project .",
        "build": "tsc",
        "test": "npm run build && jest --verbose",
        "safe-build": "rm -rf ./build && npm run lint && npm run build && jest",
        "create-docs": "typedoc",
        "postinstall": "npm run build"
    },
    "main": "./build/src/Mida.js",
    "imports": {
        "#*": "./build/src/*.js"
    },
    "exports": {
        ".": "./build/src/Mida.js",
        "./*": "./build/src/*.js"
    },
    "dependencies": {
        "puppeteer": "8.0.0",
        "tulind": "0.8.18",
        "uuid": "8.3.2"
    },
    "devDependencies": {
        "@types/jest": "26.0.20",
        "@types/node": "12.7.2",
        "@types/puppeteer": "5.4.3",
        "@types/uuid": "8.3.0",
        "jest": "26.6.3",
        "tslint": "5.18.0",
        "typedoc": "0.20.19",
        "typescript": "4.1.3"
    }
}
